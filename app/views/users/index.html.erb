<%= turbo_frame_tag "main" do %>
  <main class="container">
    <%= render current_user%>
    <%= render "users/search_form" %>
    <div class="content">
      <% @users ||= [] %>
      <% if @users.any? %>
        <%= render @users %>
      <% else %>
        <p>No results</p>
      <% end %>
    </div>
    <%= render "layouts/navbar" %>
  </main>
<% end %>